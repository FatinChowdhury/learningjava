package grindjava;
public class Main {
	public static void main(String[] args) {
		// array = store many values in one var
		String[] cars = {"Camaro","Corvette","Tesla"};
		cars[0] = "Mustang";
		System.out.println(cars[2]);
		
		// another method
		String[] fruits = new String[3]; // how many elems in array
		fruits[0] = "bananas";
		fruits[1] = "grapes";
		fruits[2] = "dates";
		
		System.out.println(fruits[2]);
		
		// use for loop to iterate all elems of array
		for(int i=0; i<fruits.length; i++) {
			System.out.println(fruits[i]);
		}
	}
}
